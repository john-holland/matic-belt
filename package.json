{
  "name": "memory-examiner",
  "version": "1.0.0",
  "description": "Memory examination and visualization system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:perf": "jest --testMatch='**/*.perf.test.ts'",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "example": "ts-node examples/basic.ts",
    "haildwarf": "haxe -main dwarf.Dwarf -js app/dwarf/Dwarf.js && node app/dwarf/Dwarf.js"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.17.0",
    "eventemitter3": "^5.0.1",
    "mongodb": "^6.3.0",
    "memory-usage": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
} 